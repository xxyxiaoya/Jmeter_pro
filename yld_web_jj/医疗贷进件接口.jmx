            <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell断言" enabled="true">
              <stringProp name="BeanShellAssertion.query">import com.APPEncode;
import net.sf.json.JSONObject;

String res = prev.getResponseDataAsString();
//请求数据解密
try{
JSONObject jstr = APPEncode.getDecode(res);
System.out.println(&quot;-------解密后的返回值--------&quot;);
System.out.println(jstr);

String code = jstr.get(&quot;code&quot;);
String checkCode = &quot;0000&quot;;// 断言code值
if(code.equals(checkCode)){
	Failure = false;
	FailureMessage = &quot;成功&quot;;	
}else{
	Failure = true;
	FailureMessage  = &quot;返回code值错误；返回code值为：&quot;+code+&quot;; 应为：&quot;+checkCode;	
}
}catch(Exception e){
	System.out.println(e);
	Failure = true;
	FailureMessage  = &quot;返回值解密错误&quot;;	
}
System.out.println(&quot;=====================================================&quot;);</stringProp>
